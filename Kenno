  // Function to open new window containing window_src, 100x400 
 function openWindow(window_src) 
  { 
  var username = "";
  var node = getElementsByClassNameTCD("logged-in", 0)[0];
   if(node != undefined)
   {
      if(document.all)
      {
  	username = node.innerText;
      }
      else
      {
             username= node.textContent.substring(312);
      }
   }
	username = username.substring(13, username.indexOf('Edit Profile'));
	window_src = window_src + '?PlayerName=' + username;
	
    window.open(window_src, 'newwindow', config='height=625,width=850, toolbar=no, menubar=no, scrollbars=no, resizable=no,location=no,  directories=no, status=no'); 
  }

  function getElementsByClassNameTCD(classname, node)  {
    if(!node) node = document.getElementsByTagName("body")[0];
    var a = [];
    var re = new RegExp('\\b' + classname + '\\b');
    var els = node.getElementsByTagName("*");
    for(var i=0,j=els.length; i<j; i++)
   {
        if(re.test(els[i].className))
        {
             a.push(els[i]);
        }
   }
    return a;
} 

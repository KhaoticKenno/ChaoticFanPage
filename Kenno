var b_version = navigator.appVersion;
var version = parseFloat( b_version );
var is_explorer7 = false;
var is_explorer8 = false;
var position = b_version.indexOf( "MSIE 7.");
var position2 = b_version.indexOf("MSIE 8.");
if( position != -1 ){
	is_explorer7 = true;
	}
if( position2 != -1 ){
	is_explorer8 = true;
	}
if ( browser == "Microsoft Internet Explorer" ){
	if ( version >= 5.5 || is_explorer7 || is_explorer8 ){
		// alert("Your browser is good enough!" + version );
	}else{
		alert("Your browser is not good enough!\n Your version:" + version + ' has to be upgraded to view this site.\nYou will be redirected to the site where you can upgrade your browser' );
		location = 'https://web.archive.org/web/20110701163417/http://www.microsoft.com/windows/downloads/ie/getitnow.mspx';
	}
}

function GetMegaRoarPost(target, postNumber, contentID)
{
    document.getElementById('ReadMoreBtn'+postNumber).style.display='none';
    if (postNumber == 1 && MegaRoar_PreLoad.length != 13)
    {
        document.getElementById(target).innerHTML=MegaRoar_PreLoad;
        return;
    }
    var callAjax = new loadXMLDoc('MegaRoarArchives.aspx','GetBlogPostContent='+contentID, target, MegaRoar_HandleResults, MegaRoar_HandleError);
    callAjax.sendRequest();
}

function MegaRoar_HandleResults(resultDest, results, params, ajaxObject) 
{
    try {
        document.getElementById(resultDest).innerHTML=results;
    }
    catch(Error) {}
}

function MegaRoar_HandleError(resultDest, params, ajaxObject) 
{
    try {
        document.getElementById(resultDest).innerHTML='Request for '+resultDest+' data failed:\n'+ajaxObject.errorText;
    }
    catch (Error) {}
}

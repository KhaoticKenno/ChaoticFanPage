</script><script language="javascript" type="text/javascript">
var Msg_Show = 'Show';
var Msg_Hide = 'Hide';
var Msg_EmailError = 'Invalid email address.';

function HideMessage() 
{
  var msgTitle = document.getElementById('SysMsgTitle');
  var showMsg = true;
  if (msgTitle.style.display == 'none') {
    showMsg = false;
  }
  try {
    var callAjax = new loadXMLDoc('_MessageReceived.aspx','ShowMsg='+ showMsg, 'HideMsg', Message_HandlePageResults, Message_HandlePageError);
    callAjax.sendRequest();
  } 
  catch (ex) {
    Message_HandlePageResults('HideMsg');
  }
}

function ResendEmail(msgType)
{
  var resendAddress = document.getElementById('ResendAddress'+msgType);
  var resendEmail = null;
  if (resendAddress != null) {
    resendEmail = resendAddress.value;
  }
  try {
  if (resendEmail != null && resendEmail != '') {
    var callAjax = new loadXMLDoc('_MessageReceived.aspx','ResendEmail='+resendEmail+'&ResendMsgType='+msgType, 'Resend', Message_HandlePageResults, Message_HandlePageError);
    callAjax.sendRequest();
  } else {
    var results = document.getElementById('ResendResult'+msgType);
    if (results != null) {
      results.innerHTML = Msg_EmailError;
      results.style.color = 'red';
    }
  }
  }
  catch (ex) { 
    var results = document.getElementById('ResendResult'+msgType);
    if (results != null) {
      results.innerHTML = "Not available on this page.";
      results.style.color = 'red';
    }
  }
}

function Message_HandlePageResults(resultDest, results, params, ajaxObject) 
{
  if (resultDest == 'HideMsg') {
  var msgTitle = document.getElementById('SysMsgTitle');
  var showLink = document.getElementById('ShowMsgLink');
  var msgContent = document.getElementById('SysMsgContent');
  if (msgTitle != null && showLink != null && msgContent != null) {
    if (msgTitle.style.display == 'none') {
      msgTitle.style.display = 'inline';
      msgContent.style.display = 'none';
      showLink.innerHTML = Msg_Show;
    } else {
      msgTitle.style.display = 'none';
      msgContent.style.display = 'inline';
      showLink.innerHTML = Msg_Hide;
    }
  }
  }
  if (resultDest == "Resend") {
    var msgType = getMsgType(params);
    var resultLbl = document.getElementById('ResendResult' + msgType);
    if (resultLbl!= null) {
      var resultList = results.split('|');
      if (resultList[0] == 'true' || resultList[0] == 'false') {
        if (resultList[1] == '[ERROR_NOTVALIDEMAIL]') {
          resultLbl.innerHTML = Msg_EmailError;
        } else {
          resultLbl.innerHTML = resultList[1];
        }
        if (resultList[0] == 'true') {
          resultLbl.style.color = 'green';
        } else {
          resultLbl.style.color = 'red';
        }
      } else {
        resultLbl.innerHTML = 'Invalid results';
        resultLbl.style.color = 'red';
      }
    }
  }
}

function Message_HandlePageError(resultDest, params, ajaxObject) 
{
  if (resultDest == "Resend") {
    var msgType = getMsgType(params);
    var resultLbl= document.getElementById('ResendResult' + msgType);
    if (resultLbl!= null) {
      resultLbl.innerHTML = 'Error sending request';
      resultLbl.style.color = 'red';
    }
  }
}

function getMsgType(params)
{
  var msgTypePos = params.indexOf('ResendMsgType=');
  if (msgTypePos >= 0) {
    var msgType = params.substring(msgTypePos + 14);
    return parseInt(msgType);
  } else {
    return 0;
  }
}
